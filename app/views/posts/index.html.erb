<h3>スケジュール一覧</h3>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>タイトル</th>
      <th>開始日</th>
      <th>終了日</th>
      <th>終日</th>
      <th>更新日時</th>
      <th>詳細ページへのリンク</th>
      <th>編集ページへのリンク</th>
      <th>削除リンク</th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.introduction %></td>
        <td><%= user.updated_at %></td>
        <td><%= user.created_at %></td>
      </tr>
    <% end %>

    <p><%= link_to "新規追加", :new_post %></p>
  </tbody>
</table>

